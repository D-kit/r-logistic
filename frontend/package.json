  "main": "src/index.js",
  "scripts": {
    "dev": "rimraf target/public/assets/ && cross-env NODE_ENV=dev webpack --config webpack.dev.config.js",
    "prod": "rimraf target/public/assets/ && cross-env NODE_ENV=production webpack -p --config webpack.production.config.js ",
    "build": "rimraf target/public/assets/ && cross-env NODE_ENV=production webpack -p --config webpack.production.config.js ",
    "start": "cross-env NODE_ENV=dev node server.hot.reload",
    "serve-dev": "npm run dev && cross-env NODE_ENV=dev nodemon ./server/index.js",
    "serve-prod": "npm run prod && cross-env NODE_ENV=production node ./server/index.js"
  },
